{
  "name": "vue-boilerplate",
  "version": "1.2.4",
  "description": "Grabarz & Partner Boilerplate",
  "author": "Stephan Gerbeth",
  "license": "MIT",
  "engines": {
    "node": ">=9.4.0"
  },
  "main": "index.js",
  "standard-version": {
    "scripts": {
      "postcommit": "git commit --amend -m \"$(git log -1 --pretty=%B) [ci skip]\""
    }
  },
  "scripts": {
    "postinstall": "npm run favicon",
    "test": "npm run lint",
    "dev": "cross-env NODE_ENV=development node ./env/nuxt/server.js",
    "build": "cross-env NODE_ENV=build nuxt build --config-file env/nuxt/config.js",
    "start": "cross-env NODE_ENV=production node ./env/nuxt/server.js",
    "static": "npm run build && cross-env NODE_ENV=static nuxt generate --config-file env/nuxt/config.js",
    "lint": "eslint --fix --ext .js,.vue --ext .json --ignore-path .gitignore ./",
    "browserslist": "npx browserslist",
    "favicon": "node ./env/tools/favicons.js",
    "release": "standard-version",
    "wdio": "npx wdio ./test/wdio/wdio.conf.js",
    "wdio-local": "npx wdio ./test/wdio/wdio.conf.local.js",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm run lint"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "babel-eslint": "^8.2.1",
    "babel-preset-vue-app": "^2.0.0",
    "browserslist-saucelabs": "^0.3.0",
    "chai": "^4.1.2",
    "cheerio": "^1.0.0-rc.2",
    "chromedriver": "^2.36.0",
    "cross-env": "^5.1.3",
    "deepmerge": "^2.0.1",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mocha": "^4.12.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.2.2",
    "favicons": "^4.8.6",
    "fs-extra": "^5.0.0",
    "geckodriver": "^1.10.0",
    "httpolyglot": "^0.1.2",
    "husky": "^0.14.3",
    "imagemin-jpeg-recompress": "^5.1.0",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-webp": "^4.1.0",
    "imagemin-webpack-plugin": "^2.0.0",
    "mocha": "^5.0.4",
    "mocha-htmllint": "^1.4.2",
    "node-notifier": "^5.2.1",
    "opn": "^5.2.0",
    "parallel-mocha": "0.0.7",
    "postcss-modules": "^1.1.0",
    "postcss-nested": "^3.0.0",
    "postcss-normalize": "^4.0.0",
    "rucksack-css": "^1.0.2",
    "selenium-standalone": "^6.13.0",
    "standard-version": "^4.3.0",
    "strip-ansi": "^4.0.0",
    "svg-sprite-loader": "^3.6.2",
    "svgo": "^1.0.4",
    "svgo-loader": "^2.1.0",
    "text-table": "^0.2.0",
    "validate-commit-msg": "^2.14.0",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-sauce-service": "^0.4.8",
    "wdio-screenshot": "^0.6.0",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.4",
    "webdriverio": "^4.12.0",
    "webpack-monitor": "^1.0.14",
    "webpack-virtual-modules": "^0.1.8"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.1.1",
    "nuxt": "^1.3.0",
    "vue-i18n": "^7.4.2",
    "vue-i18n-extensions": "^0.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GrabarzUndPartner/gp-vue-boilerplate.git"
  }
}
